-- SELECT using product and inventory tables
-- To see the quantity of products on inventory to know where I need to supply more.

select quantity, p.name
from inventory i
join product p
on p.product_id = i.product_id
order by p.name;



-- UPDATE a customer's shipping address because they moved 
-- SHP31069587 to update but first add a state in this case Ohio then create the new Shipping ID

INSERT INTO `drums`.`state`
(`state_id`,
`state`)
VALUES
(9,
'Ohio');

INSERT INTO `drums`.`shipping`
(`shipping_id`,
`zip_code`,
`apartment`,
`city`,
`shipper_id`,
`state_id`)
VALUES
('SHP31069588',
43201,
1518,
'Columbus',
201,
 9);



UPDATE `drums`.`shipping`
SET
`shipping_id` = 'SHP31069587'

WHERE `shipping_id` = 'SHP31069588';

-- DELETE
-- Delete a product because it's sold out and the manufacturer no longer provides it

select *
from product;

DELETE FROM `drums`.`product`
WHERE product_id = 647139553269;

select *
from product 
where product_id = 647139553269;

select * 
from shipping;

-- Create a view for a quick view on the customers' information for an order including full name name, city, and shipping

Create view customer_quick_view as 
select concat(cu.first_name, ',' , cu.last_name) as full_name, city, o.shipping_id
from shipping sh
join `order` o on sh.shipping_id = o.shipping_id
join customer cu on o.customer_id = cu.customer_id;

select *
from customer_quick_view;

