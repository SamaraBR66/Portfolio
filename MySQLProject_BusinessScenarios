-- SQL SCRIPTS 

create view  order_review as
select first_name, last_name, order_id
from `order` o
join customer cu
on o.customer_id = cu.customer_id
where first_name = 'Marco';

select *
from order_review;

-- sum amount product and order details join on product id

SELECT SUM(p.price) AS total_cost
FROM product p
JOIN order_details od ON p.product_id = od.product_id
WHERE p.product_id IN (622537088797, 50813172);

-- By order id


SELECT SUM(p.price * od.quantity) AS total_cost
FROM order_details od
JOIN product p ON od.product_id = p.product_id
WHERE od.order_id = 'or10';

-- Look for a product in the search bar 



SELECT product_id, `name`, price
FROM product
WHERE `name` LIKE '%sticks%';

-- Business scenarios


SELECT product_id, `name`,
CASE
    WHEN stock_status= 'out of stock' THEN 'Restock'
    ELSE 'Enough Items'
END Inventory_status
FROM product
ORDER BY `name` asc;

-- Amount sold from april to july 2023

select `name`, price 
  from product p1
  where
  price IN (select MAX(price)
  FROM  product p2
  where p2.product_id = p1.product_id
  group by p2.price);

-- Find the most expensive item in the sotre


SELECT `name`, price, category_id
FROM product
WHERE category_id = 1 
AND price = (SELECT MIN(price) 
FROM product WHERE category_id = 1);

-- See all orders from july 2023

select od.order_id, o.`date`
from order_details od
join `order` o 
on od.order_id = o.order_id
WHERE o.`date` BETWEEN '2023-07-01' AND '2023-07-22'; 


-- Create view to see what staff member is in charge of what order(s)
Create view staff_orders AS
select o.order_id, o.`date`, st.first_name
from `order` o
join staff st
on o.staff_id = st.staff_id
order by order_id ;

select * 
from staff_orders;

